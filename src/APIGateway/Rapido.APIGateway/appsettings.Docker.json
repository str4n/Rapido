{
  "app": {
    "name": "API Gateway",
    "version": "v1"
  },
  "swagger": {
    "enabled": true,
    "title": "ApiGateway",
    "version": "v1",
    "route": "swagger"
  },
  "auth": {
    "ValidIssuer": "rapido",
    "ValidAudience": "rapido",
    "Issuer": "rapido",
    "Audience": "rapido",
    "SigningKey": "zf8IlSAFEgsdfHOP564IUHH3xzaA7zaX1",
    "Expiry": "07.00:00:00",
    "validateAudience": true,
    "validateIssuer": true,
    "validateLifetime": true
  },
  "rabbitMQ": {
    "Host": "rabbitmq://rapido.rabbitmq:5672",
    "Username": "guest",
    "Password": "guest",
    "Durable": true
  },
  "logger": {
    "Level": "information",
    "Console": {
      "Enabled": true,
      "Template": "{Timestamp:HH:mm:ss} [{Level:u3}] {Message}{NewLine}{Exception}"
    },
    "Seq": {
      "Enabled": false,
      "ConnectionString": "localhost:5341"
    }
  },
  "cors": {
    "enabled": true,
    "allowCredentials": true,
    "allowedOrigins": [
      "http://localhost:4200",
      "http://localhost:5555"
    ],
    "allowedMethods": [
      "POST",
      "PUT",
      "DELETE"
    ],
    "allowedHeaders": [
      "Content-Type",
      "Authorization"
    ],
    "exposedHeaders": [
      "Resource-ID"
    ]
  },
  "consul": {
    "host": "rapido.consul",
    "discovery": {
      "Register": true,
      "ServiceName": "api-gateway",
      "Hostname": "rapido.api-gateway",
      "Port": 5000,
      "healthCheckCriticalTimeout": "1m"
    }
  },
  "tracing": {
    "enabled": true
  }
}